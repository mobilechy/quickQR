{
 "name": "QR_Man",
 "description": "Give u QR Code Directly",
 "version": "2",
 "permissions": ["tabs", "http://*/*", "background"],
 "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
 "content_scripts": [{"matches": ["http://pad.zap.jp.net/*"],"js": ["jquery.min.js","inject.js"]}],
 "browser_action": {"default_icon": "icon.png","popup": "popup.html"},
 "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
 "manifest_version": 2
}
