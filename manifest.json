{
 "name": "QR_Man",
 "description": "Give u QR Code Directly",
 "version": "3",
 "permissions": ["tabs", "http://*/*", "background"],
 "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
 "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
 "manifest_version": 2
}
